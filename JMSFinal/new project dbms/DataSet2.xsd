                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CUSTOMER.NAME, TAKEORDER.ORDERDATE, CUSTOMER.CUSTOMERID, TAKEORDER.ORDERID, PRODUCT.PRODUCTNAME, PRODUCT.FORMULA, 
                         PRODUCT.INSTRUCTION, PRODUCT.EXPIRYDATE, PRODUCT.MANUFACTUREDATE, PRODUCT.RETAILPRICE, ORDEREDPRODUCTS.QUANTITY, 
                         ORDEREDPRODUCTS.TOTALAMOUNT, ORDEREDPRODUCTS.DISCOUNT, ORDEREDPRODUCTS.GROSSAMOUNT, PAYMENTBYCUSTOMER.PAIDAMOUNT, 
                         PAYMENTBYCUSTOMER.BALANCE, PAYMENTBYCUSTOMER.STATUS
FROM            PRODUCT INNER JOIN
                         ORDEREDPRODUCTS ON PRODUCT.PRODUCTID = ORDEREDPRODUCTS.PRODUCTID INNER JOIN
                         TAKEORDER ON ORDEREDPRODUCTS.ORDERID = TAKEORDER.ORDERID INNER JOIN
                         CUSTOMER ON TAKEORDER.CUSTOMERID = CUSTOMER.CUSTOMERID INNER JOIN
                         PAYMENTBYCUSTOMER ON TAKEORDER.ORDERID = PAYMENTBYCUSTOMER.ORDERPRODUCTID AND 
                         CUSTOMER.CUSTOMERID = PAYMENTBYCUSTOMER.CUSTOMERID
WHERE        (TAKEORDER.ORDERID = @ordnum)</CommandText>
